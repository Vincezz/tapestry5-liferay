<body id="homepage">
	<div class="wrapper bs">
		<ul class="nav">
				<li>
					<t:pagelink t:page="index">Home</t:pagelink>
				</li>
				<li>
					<a href="https://github.com/got5/tapestry5-jquery">Download</a>		
				</li>
				<li>
					<a href="http://twitter.com/#!/GOTapestry5">Twitter</a>
				</li>
				<li class="last">
					<a href="https://groups.google.com/forum/?hl=fr#!forum/tapestry5-jquery">Google Group</a>
				</li>
			
		</ul>

		<div id="top">

			<h1><img src="${context:/css/layout/tapestry.png}" alt="Tapestry" id="tapestry" /></h1>

			
			
			<div id="aside">
			Tapestry5 jQuery provides jQuery Components and can fully replace Prototype and Scriptaculous by jQuery
			</div>
			
			
			<div class="clearer"></div>

		</div>
		
		<div id="bar">
			<strong>${title}</strong>
		</div>
		
		<div id="wrapper">
		<div id="menu">
			<t:jquery.accordion  t:listOfElements="prop:list" t:activeElementId="activeElement" />
				
			<t:block id="block1">
			<!-- Core Components and features -->
			<ul>
        		<li>
            		<t:pagelink t:page="jquery/Zone">Zone</t:pagelink>
        		</li>
        		<li>
            		<t:pagelink t:page="jquery/Calendar">Calendar</t:pagelink>
        		</li>
        		<li>
            		<t:pagelink t:page="jquery/Validation">Validation</t:pagelink>
        		</li>
        		<li>
            		<t:pagelink t:page="jquery/Grid">Grid</t:pagelink>
        		</li>
        		<li>
            		<t:pagelink t:page="jquery/Ajaxformloop">AjaxFormLoop</t:pagelink>
        		</li>
        		<li>
            		<t:pagelink t:page="jquery/Formfragment">FormFragment</t:pagelink>
        		</li>
        		<li>
            		<t:pagelink t:page="jquery/Linksubmit">LinkSubmit</t:pagelink>
        		</li>	
        		<li>
            		<t:pagelink t:page="jquery/JQueryAutocomplete">j:Autocomplete</t:pagelink>
        		</li>
        		<li>
                    <t:pagelink t:page="jquery/JqueryPalette">j:Palette</t:pagelink> 
        		</li>
			</ul>
			</t:block>
		
			<t:block id="block2">
			<!-- Core Components that need lib mapping -->
			<ul>
				 <li>
			         <t:pagelink t:page="jquery/JqueryDialog">Dialog</t:pagelink>
			     </li>
			     <li>
			         <t:pagelink t:page="jquery/JqueryTabs">Tabs</t:pagelink>
			     </li>
			     <li>
			         <t:pagelink t:page="jquery/JqueryAccordion">Accordion</t:pagelink>
			     </li>
			     <li>
			        <t:pagelink t:page="jquery/JquerySlider">Slider</t:pagelink>
			     </li>
			     <li>
			     	<t:pagelink t:page="jquery/AjaxuploadTest">AjaxUpload</t:pagelink>
			     </li>			
			     <li>
			     	<t:pagelink t:page="jquery/CarouselPage">Carousel</t:pagelink>
			     </li>
			     <li>
			     	<t:pagelink t:page="jquery/Checkbox">Checkbox</t:pagelink>
			     </li>
			     <li>
			     	<t:pagelink t:page="jquery/Superfish">Superfish Menu</t:pagelink>
			     </li>
			     <li>
			     	<t:pagelink t:page="jquery/DataTables">DataTables</t:pagelink>
			     </li>
			</ul>
		
			</t:block>
		
			<t:block id="block3">
		
				<!--  Exclusives jQuery components -->
			   	
			    <ul>
			   		<li>
			     		<t:pagelink t:page="jquery/Bind">Bind</t:pagelink>
			     	</li>
			    	<li>
			     		<t:pagelink t:page="jquery/Button">Button</t:pagelink>
			     	</li>
			     	<li>
			     		<t:pagelink t:page="jquery/CustomDatepicker">CustomDatepicker</t:pagelink>
			     	</li>
			       	<li>
			            <t:pagelink t:page="jquery/Mask">Mask</t:pagelink>
			        </li>
			        <li>
			            <t:pagelink t:page="jquery/reveal">Reveal</t:pagelink>
			        </li>
			        <li>
			            <t:pagelink t:page="jquery/Tooltip">Tooltip</t:pagelink>
			        </li>
			        
			    </ul>		
			</t:block>
			<t:block id="block4">
				<ul>
				     <li>
			     		<t:pagelink t:page="jquery/TwitterViewPage">TwitterViewPage</t:pagelink>
			     	</li>
			        
			    </ul>	
			</t:block>
		</div>
		
		<div id="main">
			<div id="i-component">
				<t:body />
			</div>
			<div class="clearer"></div>			
		</div>	
			
		</div>
		<div class="clearer"></div>

		<div id="footer">Created with <a href="http://tapestry.apache.org/">Apache Tapestry 5</a>, 
		Get the source code of Tapestry jQuery at <a href="https://github.com/got5">Github</a></div>

	</div>
